    "autoprefixer": "10.1.0",
    "babel-eslint": "10.1.0",
    "deepmerge": "4.2.2",
    "eslint": "7.17.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "7.1.0",
    "eslint-import-resolver-alias": "1.1.2",
